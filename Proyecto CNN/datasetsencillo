import cv2 as cv
import numpy as np

cap = cv.VideoCapture('C:\\devs\\IAProyects\\Proyecto CNN\\dataset\\VIDEOS\\porsche32.mkv')
i = 2599
while True:
    ret, frame = cap.read()
    frame = cv.resize(frame, (50,50), interpolation = cv.INTER_AREA)
    cv.imwrite('C:\\devs\\IAProyects\\Proyecto CNN\\dataset\\porsche\\vocdhfccfe3fecfesag'+str(i)+'.jpg', frame)
        
    cv.imshow('situacion', frame)
    i=i+1
    k = cv.waitKey(1)
    if k == 27:
        break
cap.release()
cv.destroyAllWindows()